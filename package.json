{
  "name": "nr-students-edition",
  "version": "1.1.0",
  "description": "[![New Relic Experimental header](https://github.com/newrelic/opensource-website/raw/master/src/images/categories/Experimental.png)](https://opensource.newrelic.com/oss-category/#new-relic-experimental)",
  "scripts": {
    "test": "NODE_ENV=test jest --passWithNoTests --coverage",
    "eslint-check": "eslint src/**/*.ts",
    "check": "ncu -e 2",
    "upgrade": "ncu -i"
  },
  "keywords": [
    "typescript",
    "Students",
    "Edition",
    "New",
    "Relic",
    "AWS",
    "Lambda",
    "Serverless"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newrelic-experimental/nr-students-edition.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/newrelic-experimental/nr-students-edition/issues"
  },
  "homepage": "https://github.com/newrelic-experimental/nr-students-edition#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/aws-lambda": "^8.10.75",
    "@types/data-api-client": "^1.2.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "npm-check-updates": "^11.5.1",
    "prettier": "^2.2.1",
    "serverless-dotenv-plugin": "^3.9.0",
    "serverless-iam-roles-per-function": "^3.1.0",
    "serverless-offline": "^6.9.0",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "aws-sdk": "2.891.0",
    "data-api-client": "^1.2.0",
    "dotenv": "^8.2.0",
    "envalid": "^7.1.0",
    "http-status-codes": "^2.1.4",
    "myzod": "^1.6.1",
    "node-fetch": "^2.6.1",
    "serverless": "^2.37.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".spec.ts$",
    "modulePathIgnorePatterns": [
      "src/types/",
      "src/functions/",
      "src/utils/org-validator.ts",
      "src/utils/status.ts",
      "src/utils/logger.ts"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-case": [
        2,
        "always",
        "lower-case"
      ],
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "feat",
          "fix",
          "refactor",
          "revert",
          "test",
          "deps"
        ]
      ],
      "scope-empty": [
        2,
        "always"
      ],
      "type-empty": [
        2,
        "never"
      ],
      "subject-empty": [
        2,
        "never"
      ],
      "subject-min-length": [
        2,
        "always",
        10
      ],
      "subject-full-stop": [
        2,
        "never",
        "."
      ],
      "subject-case": [
        2,
        "always",
        "lower-case"
      ]
    }
  }
}
